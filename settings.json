{
    "speedControl": {
        "defaultSpeed": 1.0,
        "rememberLastSpeed": false,
        "minSpeed": 0.0625,
        "maxSpeed": 10.0
    },
    "urlDefaults": [
        {
            "pattern": "youtube.com",
            "speed": 1.5
        },
        {
            "pattern": "nicovideo.jp",
            "speed": 1.25
        },
        {
            "pattern": "abema.tv",
            "speed": 1.5
        }
    ],
    "ui": {
        "showActionNotifications": true,
        "notificationDuration": 1000,
        "enableOnPageLoad": true
    },
    "keyboard": {
        "enableKeyboardShortcuts": true,
        "defaultModifiers": [
            ""
        ],
        "actions": {
            "q": {
                "type": "setSpeed",
                "speed": 1.0
            },
            "w": {
                "type": "setSpeed",
                "speed": 1.5
            },
            "e": {
                "type": "setSpeed",
                "speed": 2.0
            },
            "a": {
                "type": "increaseSpeed",
                "step": -0.25
            },
            "s": {
                "type": "increaseSpeed",
                "step": 0.0
            },
            "d": {
                "type": "increaseSpeed",
                "step": 0.25
            },
            "z": {
                "type": "increaseSpeed",
                "step": -0.1
            },
            "x": {
                "type": "increaseSpeed",
                "step": 0.1
            },
            "v": {
                "type": "seekRelative",
                "time": -2
            },
            "b": {
                "type": "seekRelative",
                "time": 2
            },
            "g": {
                "type": "seekRelative",
                "time": -6
            },
            "h": {
                "type": "seekRelative",
                "time": 6
            },
            "y": {
                "type": "seekRelative",
                "time": -18
            },
            "u": {
                "type": "seekRelative",
                "time": 18
            },
            "n": {
                "type": "seekRelative",
                "time": 80
            },
            "p": {
                "type": "seekRelative",
                "time": 36000
            },
            "\\": {
                "type": "toggle",
                "description": "拡張機能のオン/オフ切り替え"
            }
        }
    },
    "advanced": {
        "protectionMethod": "property_override",
        "debugMode": true,
        "autoApplyToNewVideos": true,
        "checkInterval": 100
    }
}