{
    "manifest_version": 3,
    "name": "動画速度コントローラー (テスト版)",
    "version": "2.1",
    "description": "テスト用の軽量動画速度・シークコントローラー",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content-test.js"
            ]
        }
    ]
}